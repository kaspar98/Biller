<script src="https://fred-wang.github.io/mathml.css/mspace.js"></script><!-- vb peaks kuidagi teisiti linkima -->

<h1>Bilanss</h1>
<span><b>Bilansi arvutamine: </b></span>
<math>
    <mrow>
        <mi>bilanss</mi>
        <mo>=</mo>
        <mi>sõbra võlg</mi>
        <mo>-</mo>
        <mi>minu võlg</mi>
    </mrow>
</math>
<div class="bilansid">
{{#each allBalances}}
    <div class="card">
        <div class="card-header">
            <div class="card-content">
                <h4>{{firstName}} {{lastName}} ({{username}})</h4>
            </div>
        </div>
        <div class="card-content">
            <h2>{{amount}} EUR</h2>
        </div>
    </div>
{{/each}}
</div>
